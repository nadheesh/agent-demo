{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Service Openapi Json",
    "version" : "0.1.0"
  },
  "servers" : [ {
    "url" : "{server}:{port}/",
    "variables" : {
      "server" : {
        "default" : "http://localhost"
      },
      "port" : {
        "default" : "8090"
      }
    }
  } ],
  "paths" : {
    "/sendEmail" : {
      "post" : {
        "summary" : "Send an email",
        "operationId" : "postSendemail",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/SendEmailPayload"
              }
            }
          }
        },
        "responses" : {
          "201" : {
            "description" : "Created",
            "content" : {
              "text/plain" : {
                "schema" : {
                  "type" : "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "SendEmailPayload" : {
        "required" : [ "content", "email", "subject" ],
        "type" : "object",
        "properties" : {
          "email" : {
            "type" : "string"
          },
          "subject" : {
            "type" : "string"
          },
          "content" : {
            "type" : "string"
          }
        }
      }
    }
  }
}